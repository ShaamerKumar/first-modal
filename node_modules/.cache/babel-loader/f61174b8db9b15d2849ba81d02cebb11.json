{"ast":null,"code":"import Modal from './components/Modal.vue';\nimport Modal2 from './components/Modal2.vue';\nexport default {\n  name: 'App',\n  components: {\n    Modal\n  },\n\n  data() {\n    return {\n      title: 'My First Vue App :)',\n      text: \"Grab for 1/2 price!\",\n      showModal: false,\n      showModal2: false\n    };\n  },\n\n  methods: {\n    handleClick() {\n      console.log(this.$refs.name);\n    },\n\n    toggleModal() {\n      this.showModal = !this.showModal;\n    },\n\n    toggleModal2() {\n      this.showModal2 = !this.showModal2;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAmCA,OAAOA,KAAP,MAAkB,wBAAlB;AACA,OAAOC,MAAP,MAAmB,yBAAnB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAC;IAACH;EAAD,CAFE;;EAGbI,IAAI,GAAG;IACL,OAAM;MACJC,KAAK,EAAE,qBADH;MAEJC,IAAI,EAAE,qBAFF;MAGJC,SAAS,EAAE,KAHP;MAIJC,UAAU,EAAC;IAJP,CAAN;EAOD,CAXY;;EAYbC,OAAO,EAAE;IACPC,WAAW,GAAE;MACXC,OAAO,CAACC,GAAR,CAAY,KAAKC,KAAL,CAAWX,IAAvB;IACD,CAHM;;IAIPY,WAAW,GAAE;MACX,KAAKP,SAAL,GAAiB,CAAC,KAAKA,SAAvB;IAED,CAPM;;IAQPQ,YAAY,GAAE;MACZ,KAAKP,UAAL,GAAkB,CAAC,KAAKA,UAAxB;IAEF;;EAXO;AAZI,CAAf","names":["Modal","Modal2","name","components","data","title","text","showModal","showModal2","methods","handleClick","console","log","$refs","toggleModal","toggleModal2"],"sourceRoot":"","sources":["/Users/shaamerk/Desktop/pract/modal-project/src/App.vue"],"sourcesContent":["<template>\n  <h1> {{title}} </h1>\n  <p> Welcome </p>\n  <div v-if=\"showModal\">\n    <Modal theme=\"sale\" @close=\"toggleModal\">\n      <template v-slot:links>\n        <a href=\"#\">sign up now</a>\n        <a href=\"#\">more info.</a>\n      </template>\n\n      <h1> Giveaway!</h1>\n\n    </Modal>\n  \n  </div>\n\n  <div v-if=\"showModal2\">\n    <Modal2 @close=\"toggleModal2\">\n      <template v-slot:links>\n        <a href=\"#\">sign up now</a>\n        <a href=\"#\">more info.</a>\n      </template>\n\n      <h1> Enter The Competition!</h1>\n\n    </Modal2>\n  \n  </div>\n  <button @click=\"toggleModal\">open modal1</button>\n  <button @click=\"toggleModal2\">open modal2</button>\n  <input type=\"text\" ref=\"name\">\n  <button @click=\"handleClick\">click me</button>\n</template>\n\n<script>\nimport Modal from './components/Modal.vue'\nimport Modal2 from './components/Modal2.vue'\n\nexport default {\n  name: 'App',\n  components:{Modal},\n  data() {\n    return{\n      title: 'My First Vue App :)',\n      text: \"Grab for 1/2 price!\",\n      showModal: false,\n      showModal2:false\n    }\n    \n  },\n  methods: {\n    handleClick(){\n      console.log(this.$refs.name)\n    },\n    toggleModal(){\n      this.showModal = !this.showModal\n\n    },\n    toggleModal2(){\n      this.showModal2 = !this.showModal2\n\n    }\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\nh1 {\n  border-bottom: 1px solid #ddd;\n  display: inline-block;\n  padding-bottom: 10px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}